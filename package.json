{
  "name": "order-services-node",
  "version": "0.0.0",
  "type": "commonjs",
  "private": true,
  "scripts": {
    "start": "node ./bin/build/src/server.js",
    "dev": "npx ts-node-dev --respawn --transpile-only ./src/server.ts",
    "build": "tsc --build",
    "create-env-example": "ts-node ./src/utils/core/generateEnvExample.ts --trace-warnings",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "npx drizzle-kit push",
    "db:seed": "ts-node ./src/db/seed/index.ts"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.44.7",
    "ejs": "^3.1.9",
    "express": "^4.21.2",
    "express-validator": "^7.3.0",
    "helmet": "^7.1.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "morgan": "~1.9.1",
    "ms": "^2.1.3",
    "mysql2": "^3.15.3",
    "node-schedule": "^2.1.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "drizzle-kit": "^0.31.7",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
